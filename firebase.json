{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "soulchat-pro",
          "appId": "1:830675250033:android:3a5cc7f90a6205811f64d0",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "soulchat-pro",
          "configurations": {
            "android": "1:830675250033:android:3a5cc7f90a6205811f64d0",
            "ios": "1:830675250033:ios:4ff7b8cf06dc37681f64d0",
            "web": "1:830675250033:web:732f79687d5ecf891f64d0"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
          { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
